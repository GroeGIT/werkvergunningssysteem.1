<%= simple_form_for(@enquirymeasure) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :enquiry %>
    <%= f.input :done %>
    <%= f.input :needed %>
    <%= f.input :responsible %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
